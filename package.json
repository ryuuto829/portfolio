{
  "name": "portfolio",
  "private": true,
  "description": "My personal portfolio",
  "version": "0.1.0",
  "author": "Dmytro Rykhlyk <dmytrorykhlyk@gmail.com>",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
