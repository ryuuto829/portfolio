    "gatsby-cli": "^2.12.117",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-plugin-styled-components": "^3.3.11",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-webfonts": "^1.1.3",
    "gatsby-remark-autolink-headers": "^2.7.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-json": "^2.4.15",
    "gatsby-transformer-remark": "^2.9.1",
    "gatsby-transformer-sharp": "^2.5.15",
    "gsap": "^3.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.30.3",
    "smooth-scroll": "^16.1.3",
    "styled-components": "^5.2.0"
